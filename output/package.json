{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm build:css && pnpm build:next",
    "build:css": "npx tailwindcss -i ./styles/input-globals.css -o ./styles/globals.css",
    "build:next": "next build",
    "dev": "concurrently npm:dev:*",
    "dev:css": "pnpm build:css --watch",
    "dev:server": "next dev",
    "lint": "next lint",
    "lint-fix": "pnpm lint --fix",
    "start": "next start"
  },
  "dependencies": {
    "next": "12.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "concurrently": "^7.2.2",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.5",
    "typescript": "4.7.4"
  }
}
