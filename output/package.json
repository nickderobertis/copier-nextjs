{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm build:css && pnpm build:next",
    "build:css": "npx tailwindcss -i ./styles/input-globals.css -o ./styles/globals.css",
    "build:next": "next build",
    "dev": "concurrently npm:dev:*",
    "dev:css": "pnpm build:css --watch",
    "dev:server": "next dev",
    "export": "pnpm build && pnpm export:next",
    "export:next": "next export",
    "lint": "next lint",
    "lint-fix": "pnpm lint --fix",
    "start-prod": "next start",
    "start-export": "http-server out",
    "storybook": "concurrently npm:dev:css npm:start-storybook",
    "start-storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.5",
    "next": "12.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.33.1",
    "react-modal": "^3.15.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "0.0.13",
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-modal": "^3.13.1",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.2.2",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.5.13",
    "http-server": "^14.1.1",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.5",
    "typescript": "4.7.4",
    "webpack": "5"
  }
}
